{
  "name": "samsonframework/view",
  "description": "SamsonFramework view rendering and generation package",
  "keywords": [
    "view",
    "rendering"
  ],
  "authors": [
    {
      "name": "Vitaly Iegorov",
      "email": "egorov@samsonos.com"
    }
  ],
  "require": {
    "samsonframework/base": "*",
    "samsonphp/generator": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "minimum-stability": "dev",
  "license": "Open Software License (OSL) v 3.0",
  "homepage": "http://samsonos.com/",
  "autoload": {
    "classmap": ["src/"],
    "psr-4": {
      "samsonframework\\view\\": "src/"
    }
  },
  "config":{
    "github-protocols":["https"]
  },
  "extra": {
    "samsonphp_package_compressable": "1",
    "samson_module_include": "1"
  }
}
